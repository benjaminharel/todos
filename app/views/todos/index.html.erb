<h1>Listing todos</h1>

<table>
  <tr>
    <th>Order</th>
    <th>Title</th>
    <th>Description</th>
    <th>Due date</th>
    <th>Checked</th>
    <th>Todo</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @todos.each do |todo| %>
  <tr>
    <td><%= todo.ind %></td>
    <td><%= todo.title %></td>
    <td><%= todo.description %></td>
    <td><%= todo.due_date %></td>
    <td><%= todo.checked %></td>
    <td><%= todo.todo_id %></td>
    <td><%= todo.user_id %></td>
    <td><%= link_to 'Show', todo %></td>
    <td><%= link_to 'Edit', edit_todo_path(todo) %></td>
    <td><%= link_to 'Destroy', todo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Todo', new_user_todo_path %>
